{
  "name": "src",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && prettier --write 'dist/**/*.html' 'dist/*.html' && find dist -type f -name '*.html' -exec sed -i '' '/^$/d' {} + && node cacheTimestamp.js",
    "format": "prettier --write dist/**/*.{html,css,js}",
    "build-and-format": "npm run build && npm run format",
    "set-fallback-true": "node fallbackImage.js true",
    "set-fallback-false": "node fallbackImage.js false",
    "build-fallback": "npm run set-fallback-true && npm run build && npm run set-fallback-false"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "glob": "^10.3.12",
    "js-beautify": "^1.15.1",
    "postcss": "^8.4.38",
    "postcss-sort-media-queries": "^5.2.0",
    "postcss-url": "^10.1.3",
    "prettier": "^3.2.5",
    "sass": "^1.74.1",
    "vite": "^5.2.0",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-imagemin": "^0.4.6",
    "vite-plugin-live-reload": "^3.0.3",
    "vite-plugin-sass-glob-import": "^3.0.2",
    "vite-plugin-webp-and-path": "^0.1.4"
  },
  "browserslist": [
    "last 2 versions",
    "> 5%",
    "iOS >= 9.0",
    "Android >= 5",
    "Firefox ESR"
  ]
}
